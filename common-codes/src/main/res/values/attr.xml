<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="smart_font" format="string"/>
    <attr name="smart_highlight_key" format="string" />
    <attr name="smart_highlight_color" format="color" />
    <attr name="smart_entries" format="reference" />
    <attr name="smart_textColor" format="color" />
    <attr name="smart_backgroundColor" format="color" />
    <attr name="smart_textSize" format="dimension" />
    <declare-styleable name="SmartTextView">
        <attr name="smart_font" />
        <attr name="smart_highlight_key" />
        <attr name="smart_highlight_color" />
    </declare-styleable>
    <declare-styleable name="SmartSpinner">
        <attr name="smart_font" />
        <attr name="smart_entries" />
        <attr name="smart_textColor" />
        <attr name="smart_backgroundColor" />
        <attr name="smart_textSize" />
    </declare-styleable>
    <declare-styleable name="SmartButton">
        <attr name="smart_font" />
    </declare-styleable>
    <declare-styleable name="SmartEditText">
        <attr name="smart_font" />
    </declare-styleable>
    <declare-styleable name="SmartCheckedTextView">
        <attr name="smart_font" />
    </declare-styleable>
    <declare-styleable name="SmartLoadingView">
        <attr name="circular_progress_colors" format="reference" />
    </declare-styleable>
</resources>